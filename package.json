{
  "name": "unite-lib",
  "version": "1.0.3",
  "description": "Assets and data library for Pokémon Unite — names, images, moves, maps. Use via npm or CDN.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./i18n": {
      "types": "./dist/i18n/index.d.ts",
      "import": "./dist/i18n/index.js",
      "require": "./dist/i18n/index.cjs"
    }
  },
  "files": [
    "dist",
    "pokemons",
    "moves",
    "maps"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "pokemon",
    "pokemon-unite",
    "unite",
    "assets",
    "data",
    "roster",
    "moves",
    "maps"
  ],
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  }
}
